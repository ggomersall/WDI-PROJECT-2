<%= form_for(@address) do |f| %>
  <% if @address.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@address.errors.count, "error") %> prohibited this address from being saved:</h2>

      <ul>
      <% @address.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="form-group">
    <%= f.label :company_name %><br>
    <%= f.text_field :company_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :telephone %><br>
    <%= f.text_field :telephone, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :address_line_1 %><br>
    <%= f.text_field :address_line_1, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :address_line_2 %><br>
    <%= f.text_field :address_line_2, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :city %><br>
    <%= f.text_field :city, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :county %><br>
    <%= f.text_field :county, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :post_code %><br>
    <%= f.text_field :post_code, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :country %><br>
    <%= f.text_field :country, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :delivery_notes %><br>
    <%= f.text_area :delivery_notes, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :user_id %><br>
    <%= f.text_field :user_id, class: "form-control" %>
  </div>
  <div class="actions form-group">
    <%= f.submit class: "btn btn-default" %>
  </div>
<% end %>
